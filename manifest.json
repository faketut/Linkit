{
  "manifest_version": 3,
  "name": "Linkit",
  "description": "",
  "version": "2.1.3",
  "action": {
    "default_popup": "pages/src/popup.html"
  },
  "options_ui": {
    "page": "pages/src/options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.linkedin.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/search/results/people/*",
        "https://*.linkedin.com/mynetwork/*",
        "https://*.linkedin.com/in/*/details/skills/*"
      ],
      "js": [
        "tab/tab.js"
      ]
    }
  ]
}